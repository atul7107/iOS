default_platform(:ios)
  lane :beta do
      ensure_git_status_clean
      increment_build_number
      commit_version_bump
      add_git_tag
      build_app(
          scheme: "test",
          clean: true
          )
      upload_to_testflight(
          app_identifier: "com.sapient.mobile.cargill"
          )
  end

  lane :screenshots do
    capture_screenshots(scheme: "test")
    frame_screenshots(white: true)
  end
end
